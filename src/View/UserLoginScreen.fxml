<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UserLoginController">
   <Button fx:id="login" layoutX="226.0" layoutY="200.0" mnemonicParsing="false" onAction="#onLogin" prefHeight="25.0" prefWidth="99.0" text="Login" />
   <Button fx:id="cancel" layoutX="337.0" layoutY="200.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="25.0" prefWidth="69.0" text="Cancel" />
   <Label fx:id="userNameLabel" layoutX="118.0" layoutY="101.0" prefHeight="17.0" prefWidth="99.0" text="UserName" textAlignment="JUSTIFY" />
   <Label fx:id="passwordLabel" layoutX="118.0" layoutY="154.0" prefHeight="17.0" prefWidth="93.0" text="Password" textAlignment="RIGHT" />
   <TextField fx:id="userNameField" layoutX="226.0" layoutY="97.0" onAction="#onUserNameField" prefHeight="25.0" prefWidth="174.0" promptText="Enter Username" />
   <TextField fx:id="password" layoutX="226.0" layoutY="150.0" onAction="#onPassword" prefHeight="25.0" prefWidth="174.0" promptText="Enter Password " />
   <Label fx:id="label" layoutX="63.0" layoutY="38.0" prefHeight="35.0" prefWidth="210.0" text="Scheduling Application Login">
      <font>
         <Font size="15.0" />
      </font>
   </Label>
   <Label fx:id="userLocationLabel" layoutX="67.0" layoutY="282.0" prefHeight="17.0" prefWidth="508.0" />
</AnchorPane>
